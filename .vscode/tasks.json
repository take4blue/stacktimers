{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "flutter",
			"command": "flutter",
			"args": [
				"test"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"label": "flutter: All Test"
		},
		{
			"type": "flutter",
			"command": "flutter",
			"args": [
				"test",
				"--update-goldens",
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"label": "flutter: golden"
		},
		{
			"type": "flutter",
			"command": "flutter",
			"args": [
				"test",
				"--coverage"
			],
			"problemMatcher": [
				"$dart-build_runner"
			],
			"group": {
				"kind": "test",
			},
			"label": "flutter: Coverage",
		},
		{
			"type": "flutter",
			"command": "flutter",
			"args": [
				"pub",
				"run",
				"build_runner",
				"build"
			],
			"problemMatcher": [
				"$dart-build_runner"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "flutter: build_runner",
		}
	]
}